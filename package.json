{
  "name": "neeko-router",
  "version": "1.0.0",
  "main": "src/router.js",
  "author": "Colin Tindle <ampersand@underscore.space>",
  "license": "CC-BY-4.0",
  "scripts": {
    "start": "parcel demo/index.html",
    "build": "parcel build demo/index.html --public-url ./",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:ci": "jest -i --coverage && cat coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "babel-jest": "^22.0.4",
    "babel-preset-env": "^1.6.1",
    "jest": "^22.0.4",
    "parcel-bundler": "^1.4.1",
    "coveralls": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/underscoredotspace/neeko-router"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "roots": [
      "src"
    ]
  }
}
